{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "lib": ["ES2020"],
  "outDir": "dist-test",
  "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["node", "mocha", "vscode"]
  },
  "include": ["test/**/*", "src/**/*"],
  "exclude": ["dist", "dist-test", "node_modules"]
}
